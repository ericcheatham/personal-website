{
  "name": "ericcheatham.dev",
  "description": "ericcheatham.dev",
  "version": "0.0.1",
  "author": "Eric Cheatham",
  "license": "MIT",
  "browserslist": "> 1% in US",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}' -p tsconfig.json && tsc",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type-check",
      "pre-write": "lint-staged && npm run type-check"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add",
      "tslint --fix -p tsconfig.json",
      "git add"
    ],
    "*.{graphql,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/styled-components": "^4.4.0",
    "gatsby": "^2.17.15",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-catch-links": "^2.1.17",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-netlify": "^2.1.25",
    "gatsby-plugin-offline": "^2.0.18",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-sharp": "^2.3.0",
    "gatsby-plugin-styled-components": "^3.1.13",
    "gatsby-plugin-typescript": "^2.1.18",
    "gatsby-remark-copy-linked-files": "^2.1.30",
    "gatsby-remark-images": "^3.1.31",
    "gatsby-remark-smartypants": "^2.1.16",
    "gatsby-source-filesystem": "^2.1.37",
    "gatsby-transformer-remark": "^2.6.35",
    "gatsby-transformer-sharp": "^2.3.5",
    "gatsby-transformer-sqip": "^2.1.44",
    "grommet-icons": "^4.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.4",
    "typeface-poppins": "0.0.72"
  },
  "devDependencies": {
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-styled-components": "^1.10.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.7.2"
  }
}
